,WAAA,cAAA,GAAA,EAAA;AACA,KA3BA;AA4BA,IAAA,kBA5BA,gCA4BA;AACA,WAAA,MAAA,GAAA,KAAA;AACA;AA9BA,GAPA;AAuCA,EAAA,KAAA,EAAA,EAvCA;AAwCA,EAAA,IAxCA,kBAwCA;AACA,WAAA;AACA,MAAA,cAAA,EAAA,KADA;AAEA,MAAA,SAAA,EAAA,EAFA;AAGA,MAAA,aAAA,EAAA,IAHA;AAIA,MAAA,cAAA,EAAA,EAJA;AAKA,MAAA,MAAA,EAAA;AALA,KAAA;AAOA;AAhDA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <div id=\"EmployeeList\">\r\n            <div class=\"list-header\">\r\n                <div class=\"list-header-left\">Nhân viên</div>\r\n                <div class=\"list-header-right\">\r\n                    <div class=\"btn-default\" @click=\"btnAddOnClick()\">Thêm mới nhân viên</div>\r\n                </div>\r\n            </div>\r\n            <div class=\"backgroud-table\" >\r\n                 <div class=\"table-kit\">\r\n                     <div class=\"kit\">\r\n                         <div class=\"kit-filter\">\r\n                             <input class=\"filter-input\" placeholder=\"Tìm theo mã, tên nhân viên\"/>\r\n                             <div class=\"ic ic-max find-icon\"></div>\r\n                         </div>\r\n                            <div class=\"btn-reload ic ic-max reload-icon\"></div>\r\n                     </div>\r\n                 </div>\r\n                <div class=\"list-table\">\r\n                    <table id=\"tblListEmployee\" class=\"table\" width=\"100%\"  border=\"0\">\r\n                        <thead>\r\n                            <tr>\r\n                                <th><input class=\"tb-checkbox\" type=\"checkbox\"/></th>       \r\n                                <th>MÃ NHÂN VIÊN</th>\r\n                                <th>TÊN NHÂN VIÊN</th>\r\n                                <th>GIỚI TÍNH</th>\r\n                                <th>NGÀY SINH</th>\r\n                                <th>SỐ CMND</th>\r\n                                <th>CHỨC DANH</th>\r\n                                <th>TÊN ĐƠN VỊ</th>\r\n                                <th>SỐ TÀI KHOẢN</th>\r\n                                <th>TÊN NGÂN HÀNG</th>\r\n                                <th>CHI NHÁNH TK NGÂN HÀNG</th>\r\n                                <th>CHỨC NĂNG</th>\r\n                                \r\n                            </tr>\r\n                            \r\n                        </thead>\r\n                        <tbody>\r\n                            <tr \r\n                                v-for=\"employee in employees\"\r\n                                :key=\"employee.employeeId\"\r\n                                @click=\"GetEmployeeIdClick(employee.employeeId)\"\r\n                                @dblclick=\"ShowEmployeeDetail(employee.employeeId)\"\r\n                                v-bind:class=\" {employeeClick: employee.employeeId === employeeClick }\"\r\n                            >\r\n                            <th><input class=\"tb-checkbox\" type=\"checkbox\"/></th>       \r\n                                <th>{{employee.employeeCode}}</th>\r\n                                <th>{{employee.employeeName}}</th>\r\n                                <th>{{employee.genderName}}</th>\r\n                                <th>{{fomatBirthday(employee.dateOfBirth)}}</th>\r\n                                <th>{{employee.identificationId}}</th>\r\n                                <th>{{employee.employeePositionId}}</th>\r\n                                <th>{{employee.employeeDepartmentId}}</th>\r\n                                <th>{{employee.bankNumber}}</th>\r\n                                <th>{{employee.bankName}}</th>\r\n                                <th>{{employee.bankBranch}}</th>\r\n                                <th>CHỨC NĂNG</th>                        \r\n                            </tr>\r\n                            \r\n                        </tbody>\r\n                    </table>\r\n                </div>\r\n\r\n                <div class=\"list-footer\">\r\n                    <dir class=\"list-footer-left\">\r\n                        <div class=\"footer-text\">Tổng số: 1035 bản ghi</div>\r\n                    </dir>\r\n                    <dir class=\"list-footer-right\">\r\n                        <div class=\"footer-select-pagesize\">10 ban ghi</div>\r\n                        <div class=\"footer-select-page\">\r\n                            <div class=\"select-page-text\">Trước</div>\r\n                            <button class=\"select-page-number\"></button>\r\n                        </div>\r\n                    </dir>\r\n                </div>\r\n           </div>\r\n        </div>\r\n\r\n        <EmployeeDetail\r\n        :isShow=\"isShow\"\r\n        @hideDialog=\"HideEmployeeDetail()\"\r\n        :employee=\"employeeDetail\"\r\n        />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\"\r\nimport dayjs from \"dayjs\"\r\n\r\n//import components\r\nimport EmployeeDetail from \"./EmployeeDetail.vue\"\r\nexport default {\r\n    components:{\r\n        EmployeeDetail,\r\n    },\r\n    created(){\r\n        this. loadData();\r\n    },\r\n    methods:{\r\n        loadData(){\r\n            axios.get('https://localhost:44355/api/v1/Employee')\r\n            .then((res) =>{\r\n                this.employees = res.data;\r\n            })\r\n        },\r\n        fomatBirthday(cusbirthday) {\r\n            if (cusbirthday == null) {\r\n                return \"Không có\";\r\n            }\r\n            return dayjs(cusbirthday).format(\"DD/MM/YYYY\");\r\n         },\r\n        GetEmployeeIdClick(employeeId){\r\n            this.employeeClick = employeeId;\r\n        },\r\n        ShowEmployeeDetail(employeeId){\r\n            axios.get('https://localhost:44355/api/v1/Employee/' +employeeId )\r\n            .then((res)=>{\r\n                this.employeeDetail = res.data;\r\n                this.isShow = true;\r\n            })\r\n        },\r\n        btnAddOnClick() {\r\n            this.isShow = true;\r\n            this.dialogFormMode = \"add\";\r\n            this.employeeDetail = {};\r\n         },\r\n        HideEmployeeDetail(){\r\n            this.isShow = false;\r\n        }\r\n    },\r\n    props: [],\r\n    data(){\r\n        return{  \r\n            dialogFormMode: \"add\",\r\n            employees: [],\r\n            employeeClick: null,\r\n            employeeDetail:[],\r\n            isShow: false,\r\n        }\r\n    },\r\n}\r\n</script>\r\n<style>\r\n@import url(\"../../styles/views/employeeList.css\");\r\n\r\n.employeeClick{\r\n    background-color: #bebebe;\r\n}\r\n</style>"],"sourceRoot":"src/views/employee"}]}